{
  "name": "agile-pokr",
  "version": "18.2.50",
  "id": "ovh.fidj.agilepokr",
  "description": "simple agile poker planning",
  "author": "fidj.ovh team",
  "homepage": "https://fidj.ovh/_/agile-pokr",
  "main": "www/index.html",
  "scripts": {
    "test": "./node_modules/.bin/karma start tests/karma.unit-tests.conf.js",
    "clean": "ionic-app-scripts clean",
    "build:app": "ionic-app-scripts build",
    "build:config": "./node_modules/.bin/gulp",
    "build:ids": "./node_modules/.bin/gulp www-fidjId",
    "build": "npm run build:config && npm run build:app && npm run build:ids",
    "lint": "ionic-app-scripts lint",
    "serve": "ionic-app-scripts serve",
    "resources": "fidj-resources --ios --android --marketing && cp resources/ios/icon/icon-60.png src/assets/icon/favicon.ico",
    "ios": "npm run build && npm run resources && ionic cordova platform add ios && ionic cordova build ios",
    "android": "npm run build && npm run resources && ionic cordova platform add android && ionic cordova build android"
  },
  "devDependencies": {
    "@ionic/app-scripts": "3.2.0",
    "gulp": "^4.0.0",
    "gulp-cheerio": "^0.6.3",
    "gulp-replace": "^0.6.1",
    "gulp-util": "^3.0.8",
    "jasmine-ajax": "^3.4.0",
    "jasmine-core": "^2.99.1",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-html-reporter": "*",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-ajax": "^0.1.13",
    "karma-jasmine-html-reporter-livereload": "*",
    "typescript": "~2.9.2"
  },
  "dependencies": {
    "@angular/animations": "5.2.11",
    "@angular/common": "5.2.11",
    "@angular/compiler": "5.2.11",
    "@angular/compiler-cli": "5.2.11",
    "@angular/core": "5.2.11",
    "@angular/forms": "5.2.11",
    "@angular/http": "5.2.11",
    "@angular/platform-browser": "5.2.11",
    "@angular/platform-browser-dynamic": "5.2.11",
    "@ionic-native/core": "~4.12.0",
    "@ionic-native/network": "^4.12.2",
    "@ionic-native/splash-screen": "~4.12.0",
    "@ionic-native/status-bar": "^4.12.2",
    "@ionic/storage": "2.1.3",
    "fidj": "^2.1.7",
    "ionic-angular": "3.9.2",
    "ionicons": "3.0.0",
    "pouchdb": "^7.0.0",
    "pouchdb-adapter-cordova-sqlite": "^2.0.5",
    "rxjs": "5.5.11",
    "save-dev": "^2.0.0",
    "sw-toolbox": "3.6.0",
    "uuid": "^3.3.2",
    "zone.js": "0.8.26"
  },
  "cordova": {
    "platforms": [
      "android",
      "ios"
    ],
    "plugins": {
      "ionic-plugin-keyboard": {},
      "cordova-plugin-whitelist": {},
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-ionic-webview": {},
      "cordova-plugin-sqlite-2": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-network-information": {}
    }
  }
}
